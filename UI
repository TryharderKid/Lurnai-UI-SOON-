local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/lIlIlIlIlIlIlIlIlIllIlIlIlIlIlIlIlIlIl/Lurnai-UI-SOON-/refs/heads/main/LurnAi%20Libary"))()

local Window = Library:CreateWindow("My Script")

local MainTab = Window:CreateTab("Main")
local SettingsTab = Window:CreateTab("Settings")

MainTab:CreateButton("Click Me!", function()
    print("Button clicked!")
end)

MainTab:CreateToggle("Toggle Feature", false, function(state)
    print("Toggle state:", state)
end)

MainTab:CreateSlider("Speed", 0, 100, 50, function(value)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
end)

SettingsTab:CreateButton("Save Settings", function()
    print("Settings saved!")
end)

Library:Notify("Success", "Script loaded successfully!", 3)
