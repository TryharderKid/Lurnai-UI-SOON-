-- Initialize the library
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/lIlIlIlIlIlIlIlIlIllIlIlIlIlIlIlIlIlIl/Lurnai-UI-SOON-/refs/heads/main/LurnAi%20Libary"))()
-- Create a new window
local Window = Library.new("My Cool Script")

-- Create tabs
local MainTab = Window:AddTab("Main", "rbxassetid://ICON_ID")
local SettingsTab = Window:AddTab("Settings", "rbxassetid://ICON_ID")

-- Create a button
Library:CreateButton(MainTab, "Click Me!", function()
    print("Button clicked!")
end)

-- Create a toggle
Library:CreateToggle(MainTab, "Enable Feature", false, function(state)
    if state then
        print("Feature enabled")
    else
        print("Feature disabled")
    end
end)

-- Create a slider
Library:CreateSlider(MainTab, "Speed", 0, 100, 50, function(value)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
end)

-- Create a notification
Library:Notify("Success", "Script loaded successfully!", 3)
