-- Initialize the library
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/lIlIlIlIlIlIlIlIlIllIlIlIlIlIlIlIlIlIl/Lurnai-UI-SOON-/refs/heads/main/LurnAi%20Libary"))()

-- Create a new window
local Window = Library.new("My Cool Script")

-- Initialize the tab system
Window = Library:CreateTabSystem(Window)

-- Create tabs and store their content
local MainTab = Window:AddTab("Main")
local MainContent = MainTab.TabContent

-- Now create elements with the correct parent
Library:CreateButton(MainContent, "Click Me!", function()
    print("Button clicked!")
end)

Library:CreateToggle(MainContent, "Enable Feature", false, function(state)
    if state then
        print("Feature enabled")
    else
        print("Feature disabled")
    end
end)

Library:CreateSlider(MainContent, "Speed", 0, 100, 50, function(value)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
end)
